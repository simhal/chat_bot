		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"vitest": "^2.0.0",
		"jsdom": "^24.0.0",
		"@playwright/test": "^1.45.0"
