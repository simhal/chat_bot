# LinkedIn OAuth Configuration
LINKEDIN_CLIENT_ID=""
LINKEDIN_CLIENT_SECRET=""

# OpenAI ChatGPT API Configuration
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=""

# OpenAI Model Selection
# Options: gpt-4o, gpt-4o-mini (recommended), gpt-4-turbo, gpt-3.5-turbo
# gpt-4o-mini is cost-effective and fast, gpt-4o is more capable but costs more
OPENAI_MODEL=gpt-4o-mini

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Database Configuration
# PostgreSQL connection string
DATABASE_URL=postgresql://chatbot_user:chatbot_password@localhost:5432/chatbot

# Redis Configuration
# Redis connection URL
REDIS_URL=redis://localhost:6379/0

# JWT Configuration
# Secret key for signing JWT tokens (generate a secure random string)
# Example: python -c "import secrets; print(secrets.token_urlsafe(32))"
JWT_SECRET_KEY=""
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440
REFRESH_TOKEN_EXPIRE_DAYS=7